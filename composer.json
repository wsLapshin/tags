{
    "name": "tjventurini/tags",
    "description": "Tags package for my laravel based homepage.",
    "keywords": ["tjventurini", "blog", "laravel", "crud", "tags"],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Venturini",
            "email": "thomas@venturini.codes"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "laravel/framework": "5.5.*",
        "backpack/crud": "^3.4.0",
        "cviebrock/eloquent-sluggable": "^4.3"
    },
    "require-dev": {
        "tjventurini/mini-bootstrap-theme": "^0.0.1"
    },
    "autoload": {
        "psr-4": {
            "Tjventurini\\Tags\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tjventurini\\Tags\\TagsServiceProvider"
            ]
        }
    }
}
